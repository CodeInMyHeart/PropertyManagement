<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.sjzc.propertymanagment.mapper.AdminsMapper">
    <select id="findByUsername" parameterType="string" resultType="cn.edu.sjzc.propertymanagment.pojo.Admins">
        select * from admins where admin_username=#{admin_username}
    </select>
</mapper>